<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="PADIP.Sosyodemografik"   Title="Veri Formu">
    <ContentPage.Content>
        <ScrollView>
        <StackLayout BackgroundColor="White" >
                <Label x:Name="lblsorusayisi" Text="{Binding Sorusayisi}"/>
             
               

                <Label x:Name="lblad"  Text="1.Adınız Soyadınız: "  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entryad"  Text="{Binding Title}  " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblyas"  Text="2-Yaşınız"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entryyas"  Keyboard="Numeric" Text="{Binding Yas}   " PlaceholderColor="White" TextColor="White" />
               
                <Label x:Name="lblyer"  Text="3-Yaşadığınız Yer(İlçe)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickeryer" Title="{Binding Yer}" TextColor="White" TitleColor="White" SelectedItem="{Binding Yer}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Kent Merkezi</x:String>
                            <x:String>Kırsal</x:String>
                         

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>
                <Label x:Name="lblmedeni"  Text="4-Medeni Haliniz" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickermedeni" Title="{Binding Medeni}" TextColor="White" TitleColor="White" SelectedItem="{Binding Medeni}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evli</x:String>
                            <x:String>Bekar</x:String>
                            <x:String>Dul</x:String>
                            <x:String>Boşanmış</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblboy"  Text="5-Boyunuz"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entryboy" Text="{Binding Boy}  " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblkilo"  Text="6-Kilonuz"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entrykilo"  Keyboard="Numeric" Text="{Binding Kilo}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblkilogebe"  Text="7-Kaç kilo ile gebe kaldınız ?"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entrykilogebe"  Keyboard="Numeric" Text="{Binding Kilogebe}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblegitim"  Text="8-Eğitim durumunuz: " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickeregitim" Title="{Binding Egitim}" TextColor="White" TitleColor="White" SelectedItem="{Binding Egitim}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Okuryazar değil</x:String>
                            <x:String>Okuryazar</x:String>
                            <x:String>İlkokul mezunu</x:String>
                            <x:String>Ortaokul mezunu</x:String>
                            <x:String>Lise mezunu</x:String>
                            <x:String>Üniversite mezunu</x:String>
                            <x:String>Yüksek Lisans/Doktora mezunu</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblcalisma"  Text="9-Çalışma durumunuz:  " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickercalisma" Title="{Binding Calisma}" TextColor="White" TitleColor="White" SelectedItem="{Binding Calisma}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Ev hanımı</x:String>
                            <x:String>Düzenli bir işte çalışıyorum</x:String>
                            <x:String>Düzensiz olarak çalışıyorum</x:String>
                          
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>
                <Label x:Name="lblguvence"  Text="10-Sağlık güvenceniz var mı?  " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickerguvence" Title="{Binding Guvence}" TextColor="White" TitleColor="White" SelectedItem="{Binding Guvence}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Var</x:String>
                            <x:String>Yok</x:String>
                           

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>
                <Label x:Name="lblbirlikte"  Text="11-Kiminle birlikte yaşıyorsunuz? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickerbirlikte" Title="{Binding Birlikte}" TextColor="White" TitleColor="White" SelectedItem="{Binding Birlikte}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Yalnız Eş</x:String>
                            <x:String>Eş ve çocukları</x:String>
                            <x:String>Eşi, çocukları ve eşinin ailesi</x:String>
                            <x:String>Eşi çocukları ve kendi ailesi</x:String>
                            <x:String>Diğer</x:String>
                         </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbltoplam"  Text="12-Evinizde sürekli yaşayan toplam kişi sayısı"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entrytoplam"  Keyboard="Numeric" Text="{Binding Evkisi}   " TextColor="White" />

                <Label x:Name="lblgelir"  Text="13-Ailenizin toplam gelir düzeyi ne kadar? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickergelir"
                        Title="{Binding Tgelir}" TextColor="White" TitleColor="White" SelectedItem="{Binding Tgelir}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>2000 tl ve altı</x:String>
                            <x:String>2001-4000</x:String>
                            <x:String>4001-6000</x:String>
                            <x:String>6001-8000</x:String>
                            <x:String>8001-10.000</x:String>
                            <x:String>10.000 üstü</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbltopgebe"  Text="14-Toplam kaç gebelik geçirdiniz (şimdiki gebelik dahil)?"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entrytopgebe"  Keyboard="Numeric" Text="{Binding Kacgebe}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblkaccocuk"  Text="15-Yaşayan kaç çocuğunuz var?"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="enrtykaccocuk" Keyboard="Numeric" Text="{Binding Kaccocuk}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lbloludogum"  Text="16-Daha önce ölü doğum ya da düşük yaşadınız mı? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickeroludogum" Title="{Binding Odogum}" TextColor="White" TitleColor="White" SelectedItem="{Binding Odogum}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                            <x:String>Evetse 1 kere</x:String>
                            <x:String>Evetse 2 kere</x:String>
                            <x:String>Evetse 3 kere</x:String>
                            <x:String>Evetse 4 kere</x:String>
                            <x:String>Evetse 5 kere</x:String>


                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbloludogum2" Text="16b-Daha önce erkenyaşta ölen çocuğunuz oldu mu? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickeroludogum2" Title="{Binding Ococuk}" TextColor="White" TitleColor="White" SelectedItem="{Binding Ococuk}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                            <x:String>Evetse 1 kere</x:String>
                            <x:String>Evetse 2 kere</x:String>
                            <x:String>Evetse 3 kere</x:String>
                            <x:String>Evetse 4 kere</x:String>
                            <x:String>Evetse 5 kere</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbloludogum3" Text="16c-Daha önce kürtaj geçirdiniz mi? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker  x:Name="pickeroludogum3" Title="{Binding Kurtaj}" TextColor="White" TitleColor="White" SelectedItem="{Binding Kurtaj}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                            <x:String>Evetse 1 kere</x:String>
                            <x:String>Evetse 2 kere</x:String>
                            <x:String>Evetse 3 kere</x:String>
                            <x:String>Evetse 4 kere</x:String>
                            <x:String>Evetse 5 kere</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblkronik"  Text="17-Gebelik ÖNCESİNDE kronik bir hastalığınız var mıydı? (Diyabet, hipertansiyon, romatizmal hastalık, tiroid hastalığı, migren, reflü, ortopedik hastalık vb.) " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickerkronik" Title="{Binding Khastalik}" TextColor="White" TitleColor="White" SelectedItem="{Binding Khastalik}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                       
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>
                <Label x:Name="lblkronik2"  Text="18-Gebelik öncesinde kronik hastalığınız varsa, günlük yaşamınızı olumsuz etkiliyor muydu?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickerkronik2" Title="{Binding Khastalik2}" TextColor="White" TitleColor="White" SelectedItem="{Binding Khastalik2}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblgebehaf"  Text="19-Gebelik Haftanız:"  Padding="10,10,0,0" FontSize="Medium" TextColor="DeepPink" />
                <Entry x:Name="entrygebehaf"  Keyboard="Numeric" Text="{Binding Ghafta2}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblbebecins"  Text="20-Bebeğin Cinsiyeti: " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickercins" Title="{Binding Cinsiyet}" TextColor="White" TitleColor="White" SelectedItem="{Binding Cinsiyet}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Kız</x:String>
                            <x:String>Erkek</x:String>
                            <x:String>Belli Değil</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblgebelikd"   Text="21-Şu anki gebeliğinizi nasıl tanımlarsınız? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickergebelikd" Title="{Binding Gebelikd}" TextColor="White" TitleColor="White" SelectedItem="{Binding Gebelikd}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Planlı gebelik</x:String>
                            <x:String>Plansız(sürpriz) istenen gebelik</x:String>
                            <x:String>Hem plansız  hem istenmeyen gebelik</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbltedavi"  Text="22-Gebelik için herhangi bir tedavi aldınız mı?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Picker x:Name="pickertedavi" Title="{Binding Gebelikt}" TextColor="White" TitleColor="White" SelectedItem="{Binding Gebelikt}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                            </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lbltakip"  Text="23-Gebelik takiplerine düzenli gidiyor musunuz? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="picker" Title="{Binding Gebeliktakip}" TextColor="White" TitleColor="White" SelectedIndexChanged="picker_SelectedIndexChanged" SelectedItem="{Binding Gebeliktakip}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="takiplabel"  Text="23b-Tahmini kaç kez takibe gittiniz?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Entry x:Name="entrylabel"  Keyboard="Numeric" Text="{Binding Gebeliktakipkac}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblgebesag"  Text="24-Şu anki gebeliğinizde bebekle ilgili bilinen bir sağlık sorunu var mı?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickergebesag"  Title="{Binding Gebelikbebeksag}" TextColor="White" TitleColor="White" SelectedIndexChanged="pickergebesag_SelectedIndexChanged" SelectedItem="{Binding Gebelikbebeksag}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="labelgebehast" Text="24b-Evet ise hastalığı belirtiniz:" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickergebehast" Title="{Binding Gebelikbebekhas}" TextColor="White" TitleColor="White" SelectedItem="{Binding Gebelikbebekhas}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Kardiyak malformasyon (kalp hastalığı)</x:String>
                            <x:String>IUGR (gelişme geriliği)</x:String>
                            <x:String>Erken doğum</x:String>
                            <x:String>Düşük doğum ağırlığı</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblsorun" Text="25-Günlük yaşantınızda sorunlarınızı rahatça konuşabildiğiniz  insanlar var mı? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickersorun" Title="{Binding Gebelikrahat}" TextColor="White" TitleColor="White" SelectedIndexChanged="pickersorun_SelectedIndexChanged" SelectedItem="{Binding Gebelikrahat}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet var</x:String>
                            <x:String>Hayır yok</x:String>
                       
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>


                <Label x:Name="rahatlabel"  Text="25b-Varsa kaç kişi var?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Entry x:Name="rahatentry" Keyboard="Numeric" Text="{Binding Gebelikrahatkac}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblcinsiyet" Text="26-Gebelik öncesinde sizin bebeğin cinsiyetiyle ilgili tercihiniz var mıydı? (kız/erkek olmasını istiyordum)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickercinsiyet" Title="{Binding Gebelikcinsiyet}" TextColor="White" TitleColor="White"  SelectedItem="{Binding Gebelikcinsiyet}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet ve istediğim cinsiyette oldu</x:String>
                            <x:String>Evet ama istediğim cinsiyette olmadı</x:String>
                            <x:String>Hayır, cinsiyetiyle ilgili tercihim yok</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblcinsiyet2" Text="27-Gebelik öncesinde eşinizin/ailenizin bebeğin cinsiyetiyle ilgili tercihi var mıydı? (kız/erkek olmasını istiyorlardı)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickercinsiyet2" Title="{Binding Gebelikacinsiyet}" TextColor="White" TitleColor="White"  SelectedItem="{Binding Gebelikacinsiyet}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet ve istediğim cinsiyette oldu</x:String>
                            <x:String>Evet ama istediği cinsiyette olmadı</x:String>
                            <x:String>Hayır, cinsiyetiyle ilgili tercihi yok</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="lblgebelikhasta" Text="28-ŞU ANKİ gebeliğiniz sırasında ortaya çıkan bir hastalığınız oldu mu? (Birden fazla seçenek işaretleyebilirsiniz.)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickergebelikhasta" Title="{Binding Gebelikhasta}" TextColor="White" TitleColor="White"  SelectedItem="{Binding Gebelikhasta}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Hayır/Yok</x:String>
                            <x:String>Diyabet (Gebelikte yüksek şeker)</x:String>
                            <x:String>Hipertansiyon (Gebelikte yüksek tansiyon)</x:String>
                            <x:String>Gebelik zehirlenmesi (Preeklampsi)</x:String>
                            <x:String>Tiroid hastalığı (Guatr)</x:String>
                            <x:String>Migren</x:String>
                            <x:String>Plasenta Previa (Yanlış yerleşimli plasenta)</x:String>
                            <x:String>Plasenta Dekolmanı (Plasentanın erken ayrılması)</x:String>
                            <x:String>Migren</x:String>
                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>
                <Label x:Name="labelgebelikhasta"  Text="28b-ŞU ANKİ gebeliğiniz sırasında ortaya çıkan bir başka bie hastalık oldu mu?" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium"/>
                <Entry x:Name="entrygebelikhasta" Keyboard="Numeric" Text="{Binding Gebelikahastadi}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="lblpsitani" Text="29a-ŞİMDİKİ gebeliğiniz sürecinde psikiyatrik bir tanı aldınız mı? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Picker x:Name="pickerpsitani" Title="{Binding Gebelikpsihastami}" TextColor="White" TitleColor="White" SelectedIndexChanged="pickerpsitani_SelectedIndexChanged"  SelectedItem="{Binding Gebelikpsihastami}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Evet</x:String>
                            <x:String>Hayır</x:String>

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>

                <Label x:Name="textgebepsihasta" Text="29b-Evetse ŞİMDİKİ gebeliğiniz sürecinde hangi psikiyatrik tanıyı aldınız? " TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
               
                <Picker x:Name="pickergebepsihasta" Title="{Binding Gebelikpsihasta}" TextColor="White" TitleColor="White"  SelectedItem="{Binding Gebelikpsihasta}"  >
                    <Picker.ItemsSource>
                        <x:Array Type="{x:Type x:String}">
                            <x:String>Depresyon</x:String>
                            <x:String>Anksiyete bozukluğu (kaygı bozukluğu)</x:String>
                            <x:String>Bipolar bozukluk (iki uçlu duygudurum bozukluğu/manik depresif)</x:String>
                            <x:String>Şizofreni (psikoz)</x:String>
                            <x:String>Panik bozukluk (panik atak)</x:String>
                           

                        </x:Array>
                    </Picker.ItemsSource>

                </Picker>


                <Label x:Name="labelgebelikpsihastadi" Text="29c-ŞİMDİKİ gebeliğiniz sürecinde yukarda bulunmayan bir hastalığınız varsa nedir? "   TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" FlowDirection="MatchParent" />
                <Entry x:Name="entrygebelikpsihastadi"  Keyboard="Numeric" Text="{Binding Gebelikpsihastadi}   " PlaceholderColor="White" TextColor="White" />

                <Label x:Name="textgebepsited" Text="30- Şu anki gebeliğinizde psikiyatrik bir tanı  aldıysanız  bunun için bir ilaç tedavisi , EKT, TMS vb  gördünüz mü ?(birden fazla şık işaretleyebilirsiniz)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Label x:Name="lbla" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="Hayır" TextColor="DeepPink" Padding="20,10,20,0" />
                <CheckBox x:Name="checka" IsChecked="{Binding Gebecaka}" CheckedChanged="checka_CheckedChanged" HorizontalOptions="Center"/>

                <Label x:Name="lblb" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="Gebelik öncesinde" TextColor="DeepPink" Padding="20,0,20,0" />
                <CheckBox  x:Name="checkb" IsChecked="{Binding Gebecakb}" CheckedChanged="checkb_CheckedChanged"   HorizontalOptions="Center"/>

                <Label x:Name="lblc" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="Gebelikte" TextColor="DeepPink" Padding="20,0,20,0" />
                <CheckBox x:Name="checkc" IsChecked="{Binding Gebecakc}" CheckedChanged="checkc_CheckedChanged"  HorizontalOptions="Center"/>

                <Label x:Name="lbld" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="Doğum Sonrasında" TextColor="DeepPink" Padding="20,0,20,0" />
                <CheckBox   x:Name="checkd" IsChecked="{Binding Gebecakd}" CheckedChanged="checkd_CheckedChanged"   HorizontalOptions="Center"/>

                <Label x:Name="textgebepsitee" Text="31- Şu anki gebeliğinizde psikiyatrik bir tanı  aldıysanız  bunun için bir psikoterapi aldınız mı ?(birden fazla şık işaretleyebilirsiniz)" TextColor="DeepPink" Padding="10,0,10,0" FontSize="Medium" />
                <Label x:Name="lbla1" HorizontalOptions="Center" HorizontalTextAlignment="Center" Text="Hayır" TextColor="DeepPink" Padding="20,10,20,0" />
